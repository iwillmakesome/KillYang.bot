{
  "name": "killyang",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec \"npm run build --prefix client && ts-node server/app.ts\"",
    "start": "cross-env NODE_ENV=production ts-node server/app.ts",
    "build": "cross-env NODE_ENV=production \"npm install --prefix client && npm run build --prefix client\"",
    "client": "npm run dev --prefix client",
    "client-install": "npm install --prefix client"
  },
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-embed": "^0.4.15",
    "prettier-plugin-sql": "^0.18.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
